{
  "name": "SurveyMonkey",
  "description": "Enjoy SurveyMonkey Integration on YellowAnt",
  "repository": "https://github.com/apurvakhatri/surveymonkeyapp1",
  "keywords": ["python3", "django2","yellowant"],
  "env": {
    "YA_DEVELOPER_TOKEN": {
      "description": "YellowAnt Developer Token",
      "required": true
    },
    "HEROKU_APP_NAME": {
      "description": "Heroku App name",
      "required": true
    },
    "SM_CLIENT_ID": {
      "description": "SurveyMonkey Client ID",
      "required": true
    },
    "SM_CLIENT_SECRET": {
      "description": "SurveyMonkey Client Secret",
      "required": true
    },
    "SM_VERIFICATION_TOKEN": {
      "description": "SurveyMonkey Verification Token",
      "required": true
    },
    "DISABLE_COLLECTSTATIC": {
      "required": false,
      "value": "1"
    },
    "ENV": {
      "required": false,
      "value": "HEROKU"
    }

  }
}
